/* css for the booking calendar widget */
.wpsbc-container .wpsbc-calendar {
  margin-left: 0px; }
.wpsbc-container .wpsbc-legend {
  margin-left: 0px; }
  .wpsbc-container .wpsbc-legend .wpsbc-legend-color {
