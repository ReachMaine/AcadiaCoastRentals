/* styling for filter and search plugin */
.searchandfilter h4 {
  margin-top: 0px;
  margin-bottom: 0px;
  line-height: 20px;
  font-size: 1.0em; }
.searchandfilter ul {
  margin-top: 0px;
  margin-bottom: 5px; }
  .searchandfilter ul li {
    display: block;
    margin-bottom: 5px; }
  .searchandfilter ul li[data-sf-field-input-type=checkbox] label {
    padding-left: 2px; }
  .searchandfilter ul ul {
    padding-left: 8px; }
    .searchandfilter ul ul li {
      padding-top: 0px;
      padding-bottom: 0px; }

.abom-filter-2col .searchandfilter ul {
  padding-left: 5px;
  /* li>ul>li {
    width: 50%;
    float: left;
    label {
      padding-left: 4px;
    }
  }*/ }
  .abom-filter-2col .searchandfilter ul ul {
    column-count: 2; }

@media only screen and (min-width: 768px) {
  #search-filter-form-3945 ul li.sf-field-category {
    width: 33%;
    float: left; }
  #search-filter-form-3945 ul li.sf-field-tag {
    width: 66%;
    float: left; }
    #search-filter-form-3945 ul li.sf-field-tag ul {
      column-count: 3; } }
.search-filter-results {
  clear: both; }

.search-filter-results .search-filter-count-wrap .search-filter-count {
  text-align: right;
  padding-bottom: 10px; }
.search-filter-results .column-block {
  margin-bottom: 20px; }
  .search-filter-results .column-block article .article-details {
    padding-top: 10px; }
  .search-filter-results .column-block article .post-bottom-details {
    display: block; }

@media only screen and (max-width: 480px) {
  #search-filter-form-3945 ul li.sf-field-category {
    width: 100%; }
  #search-filter-form-3945 ul li.sf-field-tag {
    width: 100%; } }
/* end of styling for filter and search plugin */
